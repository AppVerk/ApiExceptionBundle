{
  "name": "app-verk/api-exception-bundle",
  "description": "catch all Symfony 3.3 or newer errors and convert it to problem json RFC7807 response",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Michał Jędraszczyk",
      "email": "m.jedrasz@gmail.com"
    }
  ],
  "keywords": [
    "problem json",
    "api-problem",
    "api exceptions"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": "^7.2",
    "symfony/symfony": "4.2.*",
    "sensio/framework-extra-bundle": "^3.0.2||^5.0.0",
    "phpunit/phpunit": "^6.2",
    "polishsymfonycommunity/symfony-mocker-container": "^1.0",
    "symfony/phpunit-bridge": "^3.0",
    "guzzlehttp/guzzle": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "AppVerk\\ApiExceptionBundle\\": ""
    }
  }
}
